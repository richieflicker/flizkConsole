{
  "name": "flizkconsole",
  "displayName": "flizkConsole - Elevate Your Debugging Experience",
  "description": "Simplify your debugging process and regain code clarity with flizkConsole, a powerful Visual Studio Code extension. Easily manage, organize, and optimize console.log statements in JavaScript, TypeScript, and TSX files. Unlock streamlined debugging with intuitive keyboard shortcuts and stay up-to-date with enhanced features. Try flizkConsole today for smarter, more efficient development.",
  "version": "0.1.9",
  "pricing": "Free",
  "icon": "images/flizk.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/richieflicker/flizkConsole.git"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "publisher": "richieflicker",
  "categories": [
    "Keymaps"
  ],
  "activationEvents": [
    "onCommand:flizkConsole.displayLogMessage",
    "onCommand:flizkConsole.commentLogMessage",
    "onCommand:flizkConsole.uncommentLogMessage",
    "onCommand:flizkConsole.removeLogMessage"
  ],
  "main": "./extension.js",
  "contributes": {
    "command": "runCommands",
    "commands": [{
      "command": "displayLogMessage",
      "title": "Display Log Message"
      },{
        "command": "commentLogMessage",
        "title": "Comment Log Message"
        },{
          "command": "uncommentLogMessage",
          "title": "Uncomment Log Message"
        },{
          "command": "removeLogMessage",
          "title": "Remove Log Message"
        }],
    "keybindings": [
      {
        "when": "editorTextFocus",
      "command": "flizkConsole.displayLogMessage",
        "key": "ctrl+alt+l",
        "mac": "ctrl+alt+l"
      },
      {
        "when": "editorTextFocus",
        "command": "flizkConsole.commentLogMessage",
        "key": "ctrl+alt+c",
        "mac": "ctrl+alt+c"
      },
      {
        "when": "editorTextFocus",
        "command": "flizkConsole.uncommentLogMessage",
        "key": "ctrl+alt+z",
        "mac": "ctrl+alt+z"
      },
      {
        "when": "editorTextFocus",
        "command": "flizkConsole.removeLogMessage",
        "key": "ctrl+alt+d",
        "mac": "ctrl+alt+d"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.3.0"
  }
}
